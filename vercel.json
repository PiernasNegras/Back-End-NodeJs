{
    "version": 2,
    "builds": [
    { "src": "swagger.js", "use": "@vercel/node" },
    {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
        "includeFiles": ["swagger-output.json"]
        }
    }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "index.js" }
    ]
}
